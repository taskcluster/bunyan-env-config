  },
  "dependencies": {
    "assume": "^1.5.2",
    "babel-compile": "^2.0.0",
    "babel-preset-taskcluster": "^3.0.0",
    "babel-runtime": "^6.26.0",
    "bunyan": "^1.8.12",
    "lodash": "^4.17.4",
    "memorystream": "^0.3.1",
    "minimatch": "^3.0.4",
    "mocha": "^4.0.1",
    "sinon": "^4.1.2",
    "slugid": "^1.1.0",
    "source-map-support": "^0.5.0"
  },
  "devDependencies": {
    "eslint-config-taskcluster": "^3.0.0"
